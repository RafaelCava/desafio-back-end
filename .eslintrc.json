{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "plugin:prettier/recommended"
    ],
    "overrides": [
    ],
    "plugins": [
        "prettier",
        "jest"
    ],
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "parser": "@typescript-eslint/parser",
    "rules": {
        "prettier/prettier": "error"
    }
}
